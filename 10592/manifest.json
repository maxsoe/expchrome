{
  "manifest_version": 2,

  "name": "Optimize SWP price display when price is 0",
  "description": "10592",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "js": [ "js/jquery-2.2.0.min.js", "contentscript.js" ],
      "matches": ["http://*/*","https://*/*"]
    }
  ],

  "web_accessible_resources": [
   "html/var01.html",
   "guestreviewfilter02.html",
   "guestreviewfilter03.html",
   "guestreviewfilter04.html",
   "guestreviewfilter05.html",
   "guestreviewfilter05outof5.html",
   "guestreviewfilter06.html",
   "guestreviewfilter07.html",
   "guestreviewfilter07outof5.html",
   "guestreviewfilter08.html",
   "guestreviewfilter08outof5.html",
   "guestreviewfilter09.html",
   "guestreviewfilter09outof5.html",
   "guestreviewfilter10.html"
 ]
}
