{
  "manifest_version": 2,

  "name": "3PP - DE Region Page",
  "description": "3PP - 8845",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "js": [ "js/jquery-2.2.0.min.js", "contentscript.js" ],
      "matches": ["http://*/*","https://*/*"]
    }
  ],

  "web_accessible_resources": [
   "html/var03.html",
   "html/search-error.html",
   "html/tabs-error.html"
 ]
}
