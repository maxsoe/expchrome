{
  "manifest_version": 2,

  "name": "Air Cap messaging back into FSR",
  "description": "8220 [EWE LEO]",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "js": [ "js/jquery-2.2.0.min.js", "contentscript.js" ],
      "matches": ["http://*/*","https://*/*"]
    }
  ],

  "web_accessible_resources": [
   "html/orbitzclose.html",
   "html/orbitzcapped.html",
   "html/cheapcashclose.html",
   "html/cheapcashcapped.html",
   "html/ebookersclose.html",
   "html/ebookerscapped.html"
 ]
}
